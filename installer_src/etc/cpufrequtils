# WARNING: this file will be replaced on board support package (linux-root-...) upgrade
ENABLE=true
MIN_SPEED=0
MAX_SPEED=0
GOVERNOR=ondemand

echo 1 > /sys/devices/system/cpu/cpufreq/boost

# Set GPU to always be at max clockspeed

GPU_MAX_SPEED=$(cat /sys/class/misc/mali0/device/devfreq/devfreq0/available_frequencies | awk '{printf $NF}')

echo "userspace" > /sys/class/misc/mali0/device/devfreq/devfreq0/governor
echo $GPU_MAX_SPEED > /sys/class/misc/mali0/device/devfreq/devfreq0/min_freq
echo $GPU_MAX_SPEED > /sys/class/misc/mali0/device/devfreq/devfreq0/max_freq
